            <FontFamily x:Key="Font-cairo-light" >cairo light, /SerialGenerator;component/fonts/cairo-font/#cairo light, Arial</FontFamily>
            <FontFamily x:Key="Font-cairo-bold" >cairo bold , /SerialGenerator;component/fonts/cairo-font/#cairo bold, Arial</FontFamily>
            <FontFamily x:Key="Font-cairo-regular" >Cairo, /SerialGenerator;component/fonts/cairo-font/#Cairo Regular 400, Arial</FontFamily>


            <Style TargetType="{x:Type TextBlock}" BasedOn="{StaticResource {x:Type TextBlock}}">
